#1

thislist = ["apple", "banana", "cherry"]
mylist = thislist.copy()
print(mylist)

#2
thislist = ["apple", "banana", "cherry"]
mylist = list(thislist)
print(mylist)

#3
list1 = ["a", "b", "c"]
list2 = [1, 2, 3]

list3 = list1 + list2
print(list3)

#4
list1 = ["a", "b" , "c"]
list2 = [1, 2, 3]

for x in list2:
  list1.append(x)

print(list1)

#5
list1 = ["a", "b" , "c"]
list2 = [1, 2, 3]

list1.extend(list2)
print(list1)

